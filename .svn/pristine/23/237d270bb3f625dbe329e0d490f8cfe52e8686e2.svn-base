            <ListView Name="ListView1" Canvas.Left="-2" Canvas.Top="212">
                <ListView.View>
                    <GridView AllowsColumnReorder="True">
                        <GridViewColumn Header="Stock" DisplayMemberBinding="{Binding FirstName}" />
                        <GridViewColumn Header="Image">
                            <GridViewColumn.CellTemplate>
                                <DataTemplate>
                                    <Image Width="30" Height="30" Source="{Binding Photo}" MouseEnter="Image_MouseEnter" MouseLeave="Image_MouseLeave"></Image>
                                </DataTemplate>
                            </GridViewColumn.CellTemplate>
                        </GridViewColumn>
                        <GridViewColumn Header="VIV" DisplayMemberBinding="{Binding SurName}" />
                        <GridViewColumn Header="Year" DisplayMemberBinding="{Binding FirstName}" />
                        <GridViewColumn Header="Mark" DisplayMemberBinding="{Binding SurName}" />
                        <!--<GridViewColumn Header="  Model" DisplayMemberBinding="{Binding FirstName}"/>-->
                        <GridViewColumn Header="Trim" DisplayMemberBinding="{Binding SurName}" />
                        <GridViewColumn Header="Video" DisplayMemberBinding="{Binding FirstName}" />
                    </GridView>
                </ListView.View>
            </ListView>
        </Border>
            <Popup Name="popup1" VerticalOffset="20" 
                                       PlacementRectangle="300,140,100,150" PopupAnimation="Fade"  
                                       AllowsTransparency="True" Margin="959,724,109,42" Placement="Mouse"  IsOpen="False">


        <Image Name="img1" Height="150" Width="200"  />

        </Popup>
        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="197,77,0,0" Name="button1" VerticalAlignment="Top" Width="75" Background="#FF656F77" Foreground="#FF888888" />
