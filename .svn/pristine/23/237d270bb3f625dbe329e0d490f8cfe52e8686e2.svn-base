            <Path.Effect>
                <DropShadowEffect BlurRadius="0" Color="#FFFFFFFF" Direction="-90" Opacity="0.3" ShadowDepth="1"/>
            </Path.Effect>
        </Path>
        <ComboBox Height="23" Margin="402,247,299,362" Name="comboBox2" Width="120" />
        <ComboBox Height="23" Margin="557,72,144,537" Name="comboBox3" Width="120" />
        <ComboBox Height="23" Margin="382,72,319,537" Name="comboBox4" Width="120" />
        <Label Content="sdfsdfdsfsdfsdfds" Height="28" HorizontalAlignment="Left" Margin="84,94,0,0" Name="label1" VerticalAlignment="Top" Width="58" />
