<Window x:Class="WpfVideoUploader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Style="{StaticResource MacWindowStyle}"
        Title="MainWindow" Height="653" Width="1197" Background="White">



    <Grid Height="632">
        <Path Margin="13,4,9,27" Opacity="0.9" Stretch="Fill" Stroke="#FF000000" Data="M0,40 C0,0 40,0 40,0 C880,0 848,0 880,0 C912,0 912,40 912,40 C912,616 912,576 912,616 C912,656 872,656 872,656 C48,656 96,656 48,656 C0,656 0,616 0,616" Canvas.Left="6" 
